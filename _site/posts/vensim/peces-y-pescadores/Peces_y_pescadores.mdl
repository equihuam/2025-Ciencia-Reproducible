mortalidad = WITH LOOKUP (
	Peces / capacidad de carga,
		([(0,5)-(1,6)],(0,5.22),(0.1,5.225),(0.2,5.23),(0.3,5.24),(0.4,5.255),(0.5,5.29),(0.6\
		,5.345),(0.7,5.45),(0.8,5.665),(0.9
		,5.94),(1,6) ))
	~	1/Year
	~		|

balance anual=
	ganancias - costos
	~	
	~		|

captura por barco = WITH LOOKUP (
	densidad,
		([(0,0)-(12,30)],(0,0),(1,5),(2,10.38),(3,15.88),(4,20.25),(5,22.13),(6,23.25),(7,23.75\
		),(8,24.25),(9,24.63),(10,25),(11,25.35),(12,25.48) ))
	~	fish/ship/Year
	~		|

ganancias anuales=
	balance anual
	~	$/Year
	~		|

nuevos barcos construidos=
	MAX(0, balance anual * inversión en nuevos barcos / costo de un barco)
	~	ship/Year
	~		|

costo de un barco=
	300
	~	
	~		|

Peces= INTEG (
	cantidad de peces nuevos - (cantidad de peces muertos + captura total anual),
		1000)
	~	fish
	~		|

ganancias=
	precio del pescado * captura total anual
	~	$/Year
	~		|

área=
	100
	~	miles*miles
	~		|

capacidad de carga=
	1200
	~	fish
	~		|

costos=
	Flota pesquera*costo de operar un barco
	~	$/Year
	~		|

densidad=
	Peces / área
	~	fish/(miles*miles)
	~		|

cantidad de peces muertos=
	mortalidad * Peces
	~	fish/Year
	~		|

cantidad de peces nuevos=
	Peces * tasa de natalidad de peces
	~	fish/Year
	~		|

precio del pescado=
	20
	~	$/fish
	~		|

inversión en nuevos barcos=
	0.2
	~	ship/Year
	~		|

tasa de natalidad de peces=
	6
	~	1/Year
	~		|

Número inicial de barcos=
	10
	~	ship
	~		|

balance financiero inicial total=
	0
	~	$
	~		|

Cantidad inicial de peces=
	1000
	~	fish
	~		|

Flota pesquera= INTEG (
	nuevos barcos construidos,
		Número inicial de barcos)
	~	ship
	~		|

captura total anual=
	Flota pesquera  * captura por barco
	~	fish/Year
	~		|

balance financiero total= INTEG (
	ganancias anuales,
		balance financiero inicial total)
	~	$
	~		|

costo de operar un barco=
	250
	~	$/ship/Year
	~		|

