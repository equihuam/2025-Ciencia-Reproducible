mortalidad = WITH LOOKUP (
	Peces / capacidad de carga,
		([(0,5)-(1,6)],(0,5.22),(0.1,5.225),(0.2,5.23),(0.3,5.24),(0.4,5.255),(0.5,5.29),(0.6\
		,5.345),(0.7,5.45),(0.8,5.665),(0.9
		,5.94),(1,6) ))
	~	1/Year
	~		|

balance anual=
	ganancias - costos
	~	
	~		|

captura por barco = WITH LOOKUP (
	densidad,
		([(0,0)-(12,30)],(0,0),(1,5),(2,10.38),(3,15.88),(4,20.25),(5,22.13),(6,23.25),(7,23.75\
		),(8,24.25),(9,24.63),(10,25),(11,25.35),(12,25.48) ))
