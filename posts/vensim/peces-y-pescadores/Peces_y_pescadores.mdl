	~	$/Year
	~		|

densidad=
	Peces / área
	~	fish/(miles*miles)
	~		|

cantidad de peces muertos=
	mortalidad * Peces
	~	fish/Year
	~		|

cantidad de peces nuevos=
	Peces * tasa de natalidad de peces
	~	fish/Year
	~		|

precio del pescado=
	20
	~	$/fish
	~		|

inversión en nuevos barcos=
	0.2
	~	ship/Year
	~		|

tasa de natalidad de peces=
	6
	~	1/Year
	~		|

Número inicial de barcos=
	10
	~	ship
	~		|

balance financiero inicial total=
	0
	~	$
	~		|

Cantidad inicial de peces=
	1000
	~	fish
	~		|

Flota pesquera= INTEG (
	nuevos barcos construidos,
		Número inicial de barcos)
	~	ship
	~		|

captura total anual=
	Flota pesquera  * captura por barco
	~	fish/Year
	~		|

balance financiero total= INTEG (
	ganancias anuales,
		balance financiero inicial total)
	~	$
	~		|

costo de operar un barco=
	250
	~	$/ship/Year
